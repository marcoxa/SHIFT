    # Setting up Tcl/Tk

    # Library paths
    # Check whether --with-libtcl-loc or --without-libtcl-loc was given.
if test "${with_libtcl_loc+set}" = set; then
  withval="$with_libtcl_loc"
  TCLLIBPATH=$withval
else
  # No argument was given.  We should set TCLLIBPATH to $libdir,
		# however the value of this variable is not yet usable.
		# Hence the following kruft
		if test "x$exec_prefix" = xNONE
		then
		    if test "x$prefix" = xNONE
		    then
			TCLLIBPATH=$ac_default_prefix/lib
		    else
			TCLLIBPATH=$prefix/lib
		    fi
		else
		    TCLLIBPATH=$exec_prefix/lib
		fi
		
fi


    test -d "$TCLLIBPATH" \
    || { echo "configure: error: --with-libtcl-loc=$TCLLIBPATH is an invalid library path" 1>&2; exit 1; }

    # Check whether --with-libtk-loc or --without-libtk-loc was given.
if test "${with_libtk_loc+set}" = set; then
  withval="$with_libtk_loc"
  TKLIBPATH=$withval
else
  TKLIBPATH=$TCLLIBPATH
fi

    test -d "$TKLIBPATH" \
    || { echo "configure: error: --with-libtk-loc=$TKLIBPATH is an invalid library path" 1>&2; exit 1; }

    # Include paths
    # Check whether --with-incltcl-loc or --without-incltcl-loc was given.
if test "${with_incltcl_loc+set}" = set; then
  withval="$with_incltcl_loc"
  TCLINCLUDEPATH=$withval
else
  # Again, no argument was given.  We should set
		# TCLLIBPATH to $include, however the value of this
		# variable isalso not yet usable.
		# Hence the following kruft
		if test "x$prefix" = xNONE
		then
		    TCLINCLUDEPATH=$ac_default_prefix/include
		else
		    TCLINCLUDEPATH=$prefix/include
		fi
		
fi

    test -d "$TCLINCLUDEPATH" \
    || { echo "configure: error: --with-incltcl-loc=$TCLINCLUDEPATH is an invalid include path" 1>&2; exit 1; }

    # Check whether --with-incltk-loc or --without-incltk-loc was given.
if test "${with_incltk_loc+set}" = set; then
  withval="$with_incltk_loc"
  TKINCLUDEPATH=$withval
else
  TKINCLUDEPATH=$TCLINCLUDEPATH
fi

    test -d "$TKINCLUDEPATH" \
    || { echo "configure: error: --with-incltk-loc=$TKINCLUDEPATH is an invalid include path" 1>&2; exit 1; }


